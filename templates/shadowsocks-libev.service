[Unit]
Description=shadowsocks-libev - lightweight secured socks5 proxy
Documentation=man:shadowsocks-libev(8)
After=network.target

[Service]
Type=simple
LimitNOFILE=32768
ExecStart=/usr/bin/ss-server -u -c {{ shadowsocks_home }}/shadowsocks-libev.json --acl {{ shadowsocks_home }}/acl.txt

[Install]
WantedBy=multi-user.target
